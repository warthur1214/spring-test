<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.warthur.spring.mybatis.dao.UserDAO">

    <select id="selectUserList" resultType="com.warthur.spring.mybatis.dto.UserDTO"
            parameterType="com.warthur.spring.mybatis.pojo.UserInfo">
        SELECT
            user_name AS userName, age
        FROM db_test.t_user
        WHERE id = ${id}
    </select>
</mapper>